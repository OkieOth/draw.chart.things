{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Additional Formats",
  "description": "Model to inject additional formats in a boxes layout definition",
  "version": "0.1.0",
  "type": "object",
  "additionalProperties": {
    "$ref": "./boxes.json#/definitions/Format"
  },
  "examples": [
    {
      "b_core": {
        "fontCaption": {
          "size": 0,
          "lineHeight": 0,
          "maxLenBeforeBreak": 300
        }
      },
      "b_ext": {
        "fill": {
          "color": "#f9bc85"
        }
      }
    }
  ]
}
